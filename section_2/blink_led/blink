S_0x155e05000 .scope module, "tb_blink_led" "tb_blink_led" 2 3;
 .timescale -9 -12;
v0x6000014d01b0_0 .var "clk", 0 0;
v0x6000014d0240_0 .net "led", 0 0, v0x6000014d0120_0;  1 drivers
S_0x155e05170 .scope module, "uut" "blink_led" 2 10, 3 2 0, S_0x155e05000;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /OUTPUT 1 "led";
v0x6000014d0000_0 .net "clk", 0 0, v0x6000014d01b0_0;  1 drivers
v0x6000014d0090_0 .var "counter", 3 0;
v0x6000014d0120_0 .var "led", 0 0;
E_0x6000033d0ec0 .event posedge, v0x6000014d0000_0;
    .scope S_0x155e05170;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000014d0120_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x155e05170;
T_1 ;
    %wait E_0x6000033d0ec0;
    %load/vec4 v0x6000014d0090_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x6000014d0090_0, 0;
    %load/vec4 v0x6000014d0090_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x6000014d0090_0, 0;
    %load/vec4 v0x6000014d0120_0;
    %inv;
    %assign/vec4 v0x6000014d0120_0, 0;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x155e05000;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6000014d01b0_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x6000014d01b0_0;
    %inv;
    %store/vec4 v0x6000014d01b0_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x155e05000;
T_3 ;
    %vpi_call 2 25 "$dumpfile", "blink.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x155e05000 {0 0 0};
    %delay 200000, 0;
    %vpi_call 2 27 "$finish" {0 0 0};
    %end;
    .thread T_3;
